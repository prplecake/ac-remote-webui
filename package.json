    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.21",
    "babel-loader": "^9.1.2",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "chart.js": "^4.4.2",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "react-strap": "^0.0.1",
    "reactstrap": "^9.2.2",
    "sass": "^1.71.1",
    "sass-loader": "^13.3.3",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.2",
    "web-vitals": "^3.5.2",
    "webpack-bundle-tracker": "^3.0.1",
    "webpack-cli": "^5.1.1"
  },
  "overrides": {
    "typescript": "^5.0.4"
  },
  "scripts": {
    "test:lint:js": "eslint src/**/*.{ts,tsx}",
    "test:lint:fix-js": "eslint src/**/*.{ts,tsx} --fix",
    "test:lint:sass": "stylelint \"src/**/*.{css,scss}\" && prettier --check \"src/**/*.{css,scss}\"",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "dev": "webpack --config webpack.config.js --watch --mode development",
    "build": "webpack --config webpack.config.js --mode production",
    "start": "webpack serve --mode development"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.6.0",
    "prettier": "^3.2.5",
    "stylelint": "^15.11.0",
    "stylelint-config-recommended-scss": "^13.1.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-scss": "^5.3.2",
    "webpack": "^5.90.3",
    "webpack-dev-server": "^4.15.1"
  }
